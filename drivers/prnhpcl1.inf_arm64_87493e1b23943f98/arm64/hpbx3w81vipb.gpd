       {
              *rcNameID: =IDS_GRAYSCALE_OFF
              *PrintSchemaKeywordMap: "Color"
              *Color?: TRUE
       }
       *Option: GrayScale
       {
              *rcNameID: =IDS_GRAYSCALE_HIGH_QUALITY
